<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<connectionStrings>
		<!--
      Update the connection string to point to your SQL Server / LocalDB instance.
	 Example using local SQL Server instance and a ContactsDB database.
    -->
		<add name="ContactsDb"
			connectionString="Data Source=localhost;Initial Catalog=ContactsDb;Integrated Security=True;TrustServerCertificate=True;"
			 providerName="System.Data.SqlClient" />
	</connectionStrings>
</configuration>
